<h1>Contact Us</h1>

<%= form_with(model: @lead, url: leads_path, method: :post) do |form| %>
  <div>
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>
  </div>

  <div>
    <%= form.submit 'Submit' %>
  </div>
<% end %>
